// RISC-V Instruction Memory
@0000

// Initialize registers
00a00093 // addi x1, x0, 10        // x1 = 10
0001a137 // lui x2, 0x1A           // x2 = 0x1A000
6d700193 // addi x3, x0, 1751      // x3 = 1751 = 0110 1101 0111 
00119193 // slli x3, x3, 1         // x3 = 3502 = 0 1101 1010 1110 = 0DAE
00310133 // add x2, x2, x3         // x2 = 0x1ADAE = 109998 (combine high and low parts 0xDAE)
00000013 // nop
00000013 // nop
00000013 // nop
00000013 // nop
00000013 // nop

// Test memory read and write
0020a123 // sw x2, 2(x1)           // mem[3] = x2 = 109998 = 01 1010 1101 1010 1110
00208283 // lb x5, 2(x1)           // x5 = mem[3][7:0] = -82
00209303 // lh x6, 2(x1)           // x6 = mem[3][15:0] = -21074
0020a383 // lw x7, 2(x1)           // x7 = mem[3] = 109998
0020c403 // lbu x8, 2(x1)          // x8 = mem[3][7:0] = 174
0020d483 // lhu x9, 2(x1)          // x9 = mem[3][15:0] = 44462
00200823 // sb x2, 16(x0)          // mem[4] = x2[7:0] = 174
00201a23 // sh x2, 20(x0)          // mem[5] = x2[15:0] = 44462
